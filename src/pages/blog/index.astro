---
import Layout from "../../layouts/Layout.astro";

const allPosts = await Astro.glob("./*.md");
---

<Layout>
    {console.log("llll", allPosts)}
    {
        allPosts.map((post) => (
            <li>
                <a href={post.url}>{post.frontmatter.title}</a>
            </li>
        ))
    }
</Layout>
