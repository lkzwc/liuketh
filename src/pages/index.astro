---
import { Image } from "astro:assets";
import Card from "../components/Card.astro";
import Layout from "../layouts/HomeLayout.astro";
import flowcarddragon from "../image/product/dragon.jpg";
import flowcardlt from "../image/product/flowcard.jpg";
import flowcardgd from "../image/product/gd.jpg";
import yl from "../image/product/yl.jpg";
import neibu from "../image/product/neibu.jpg";
import ai from "../image/index/ai.svg";
---

<Layout>
	<main>
		<section>
			<!-- Hero top -->
			<div class="bg-[#e9e9ec] flex px-60">
				<!-- Container -->
				<div>
					<Image
						src={ai}
						alt=""
						class="mt-40"
						width={500}
						height={500}
					/>
				</div>
				<div class="h-[90vh] ml-40 my-20 md:px-10 md:py-24 lg:py-32">
					<!-- Title -->
					<h1
						class="mb-6 max-w-4xl text-5xl font-bold md:mb-10 md:text-6xl lg:mb-12"
					>
						万物互联 畅想未来
					</h1>
					<!-- Buttons -->
					<div class="flex items-stretch">
						<a
							href="http://tc.liuketh.cn"
							class="mr-6 rounded-md bg-black px-8 py-4 text-center font-semibold text-white lg:mr-8"
						>
							免费领卡</a
						>
